md-toolbar: h1.md-toolbar-tools Lists
md-content
  md-list
    md-list-item( ng-repeat="list in lists" ng-click="$state.go('list', {id: list._id}); main.toggleNav('left')" )
      .md-list-item-text
        h2.md-title {{list.name}}
        p.md-subhead {{list.description}}
      md-divider( ng-if="!$last" )
md-button.md-fab.md-fab-bottom-right( ng-click="$state.go('listNew'); main.toggleNav('left')" )
  md-icon add
